{
  "expo": {
    "name": "Sinara Soon",
    "slug": "pos-kospin",
    "version": "0.0.1",
    "sdkVersion": "52.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_icon_mobile_300.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "experiments": {
      "typedRoutes": true
    },
    "splash": {
      "image": "./assets/images/logo_mobile_300.png",
      "resizeMode": "contain",
      "backgroundColor": "#0066AE"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_mobile_1024.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.abunda.poskospin"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon-32x32.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo_mobile_300.png",
          "imageWidth": 100,
          "backgroundColor": "#0066AE",
          "resizeMode": "contain"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0cfb8075-db1d-4a85-a096-b29d6d2f4aca"
      }
    },
    "owner": "abunda"
  }
}
